@model MovieDemo.Models.DemoStudent
@{
    ViewBag.Title = "Show";
}

@{
    string title = "Mr.";
    string greeting = "";
    if (Model.Gender == "F")
    {
        title = "Mrs.";
    }
    greeting = "Welcome " + title + " " + Model.LastName + " " + Model.FirstName + " From " + Model.University + " University!";
    ViewBag.Greeting = greeting;
}
<h2>@greeting</h2>

